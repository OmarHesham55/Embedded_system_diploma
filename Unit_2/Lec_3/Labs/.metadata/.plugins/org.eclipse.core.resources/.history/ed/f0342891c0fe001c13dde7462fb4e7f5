#include "stdio.h"


// calculate the average students degrees

int main(){
	 int students;
//	 float summation = 0;

	 printf("Enter numbers of students:\t");
	 fflush(stdin); fflush(stdout);
	 scanf("%d",&students);

	 for(int i=0;i<=students;i++){
		 printf("degree of student(%d)\n",i);
	 }

}
