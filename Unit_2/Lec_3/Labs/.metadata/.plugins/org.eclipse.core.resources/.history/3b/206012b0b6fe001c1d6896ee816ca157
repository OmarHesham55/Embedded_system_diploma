#include "stdio.h"

int main(){

	int x,y,z;

	printf("Enter three values you need to compare");
	fflush(stdin); fflush(stdout);
	scanf("num1: %d \n, num2: %d \n, num3: %d \n",&x, &y, &z);

	if(x>y){
		if(x>z){
			printf("X is the largest value");
		}else{
			printf("Z is the largest value");
		}
	}else{
		if(y>z){
			printf("Y is the largest value");
		}else{
			printf("Z is the largest value");
		}
	}



}
