#include "stdio.h"

int main(){
	 int val,sum=0 ;
	printf("Enter a positive integer..");
	fflush(stdin); fflush(stdout);
	scanf("%d",&val);
//	printf("%d",val);

	for(int i=0; i<val; i++){
		sum = sum+i;
		printf("%d",sum=sum+i)
	}
}
