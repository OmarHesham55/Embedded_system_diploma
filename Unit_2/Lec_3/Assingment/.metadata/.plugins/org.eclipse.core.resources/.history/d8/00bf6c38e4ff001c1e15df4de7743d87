 */


#include "stdio.h";

int main(){
	float x,y,sym;
	char symbol;
	printf("Enter first number");
	fflush(stdin); fflush(stdout);
	scanf("%f",&x);

	printf("Enter second number");
	fflush(stdin); fflush(stdout);
	scanf("%f",&y);

	printf("Enter which calculation you want");
	fflush(stdin); fflush(stdout);
	scanf("%f",&sym);

	switch(sym){
	case "+":{
		printf("%d",x+y);
	}
	break;

	case "-":{
		printf("%d",x-y);
	}
	break;

	case "*":{
		printf("%d",x*y);
	}
	break;

	case "/":{
		printf("%d",x/y);
	}
	break;
	default:{
		printf("HHHHH");
		break;
	}
	}
}
