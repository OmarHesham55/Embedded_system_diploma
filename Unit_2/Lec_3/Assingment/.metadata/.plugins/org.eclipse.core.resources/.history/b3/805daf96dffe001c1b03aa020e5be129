#include "stdio.h";

int main(){

	char alphabet;
	printf("Enter a character:");
	fflush(stdin); fflush(stdout);
	scanf("%c",&alphabet);
	switch (alphabet){
	case 'i':
	case 'I':
	{
		printf("This letter is vowel");

	}
	break;
	case 'a':
	case 'A':{
		printf("This letter is vowel");
	}
	break;

	case 'a':
	case 'A':{
		printf("This letter is vowel");
	}
	break;

	case 'e':
	case 'E':{
		printf("This letter is vowel");
	}
	break;

	case 'o':
	case 'O':{
		printf("This letter is vowel");
	}
	break;

	case 'u':
	case 'U':{
		printf("This letter is vowel");
	}
	break;

	default:{
		printf("this letter is constant");
		break;
	}

	}
}
