#include "stdio.h"

int main(){
	int arr[100],ele,i,NO,LOC;

	printf("Enter the No. of elements: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&ele);

	for(i=0; i<ele; i++){
		printf("%d ",arr[i]);
	}

	printf("Enter the No. to be inserted: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&NO);

	printf("Enter the location: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&LOC);
}
