#include"stdio.h"

 int main(){

	 float matrix1[2][2];
//	 float matrix2[2][2];
	 int i,j;

	 printf("Enter the values of the matrix1\n");


	 for(i=0; i<2; i++){
		 for(j=0; j<2; j++){
			printf("M%d%d", i,j);
			fflush(stdin); fflush(stdout);
			scanf("%f",&matrix1[i][j]);
		 }
	 }
 }
