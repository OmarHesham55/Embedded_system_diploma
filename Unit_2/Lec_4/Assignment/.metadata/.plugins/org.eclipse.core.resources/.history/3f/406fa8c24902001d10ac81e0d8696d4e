#include "stdio.h"

int main(){
	int ele,arr[50],i;

	printf("Enter the num of elements: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&ele);

	for(i=0; i<ele; i++){
		printf("element %d: ",i+1);
		fflush(stdin); fflush(stdout);
		scanf("%d",&arr[i]);
	}

	for(i=0; i<ele; i++){
		printf("%d ",arr[i]);
	}
}
