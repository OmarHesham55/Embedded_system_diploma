#include "stdio.h"

int main(){
	int ele,arr[50],i,Snum;

	printf("Enter the num of elements: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&ele);

	for(i=0; i<ele; i++){
		printf("element %d: ",i+1);
		fflush(stdin); fflush(stdout);
		scanf("%d",&arr[i]);
	}



	printf("\nEnter the num to search in elements: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&Snum);



	for(i=0; i<ele; i++){
		if(arr[i] == Snum){
			printf("the number is founded in location: %d",i+1);
		}
	}

}
