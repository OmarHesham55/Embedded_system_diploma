#include "stdio.h"

struct sStudents{
	int id;
	char name[50];
	float marks;
};

int main(){
	struct sStudents student[];

	printf("Your ID:");
	fflush(stdin); fflush(stdout);
	scanf("%d",&student.id);

	printf("Your Name:");
	fflush(stdin); fflush(stdout);
	gets(student.name);

	printf("Your Mark:");
	fflush(stdin); fflush(stdout);
	scanf("%d",&student.marks);
}
