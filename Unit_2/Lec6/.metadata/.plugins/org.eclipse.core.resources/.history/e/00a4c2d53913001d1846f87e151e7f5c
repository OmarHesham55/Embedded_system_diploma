#include "stdio.h"

struct sumDistance{
	float dist_feet1;
	float dist_inch1;
	float dist_feet2;
	float dist_inch2;
};

	int main(){
		struct sumDistance distance;

		printf("Enter first distance:");
		fflush(stdin); fflush(stdout);
		scanf("%f",distance.dist_feet1);


	}
