#include "stdio.h"

int func1(){
	int num,i,sum=1;
	printf("Enter a number: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&num);
	for(i=num; i>1; --i){
		sum *= i; // sum = sum*i
	}
	printf("%d",sum);
	return sum;
}

int main(){
	func1();

}
