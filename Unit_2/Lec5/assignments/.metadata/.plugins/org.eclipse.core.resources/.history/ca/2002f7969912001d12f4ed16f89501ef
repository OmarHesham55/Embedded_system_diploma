#include "stdio.h"

int main(){
	int base,exp,i,counter;

	printf("enter a number:");
	fflush(stdin); fflush(stdout);
	scanf("%d",&base);

	printf("enter the power:");
	fflush(stdin); fflush(stdout);
	scanf("%d",&exp);

	printf("%d",base^exp);
}
