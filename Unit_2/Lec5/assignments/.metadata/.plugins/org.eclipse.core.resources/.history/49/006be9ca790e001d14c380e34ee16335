#include "stdio.h"

	int func1(){

		int x,y,i;
		printf("Enter two numbers: ");
		fflush(stdin); fflush(stdout);
		scanf("%d %d",&x ,&y);

		if(x<y){
			for(i=x;i<y;i++){
				if(i%2 != 0){
					printf("%d",i);
				}
			}
		}else{
			for(i=y;i<x;i++){
				if(i%2 != 0 ){
					printf("%d",i);
				}
			}
		}
		return i;
	}

int main(){

	func1();
}
