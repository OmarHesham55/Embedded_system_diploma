	int num,count,i;
	printf("Enter a number: ");
	fflush(stdin); fflush(stdout);
	scanf("%d",&num);
	for(i=1;i<=num;i++){
		if(num%i == 0){
			count++;
		}
	}
	if(count == 2){
		printf("%d is a prime number",num);
	}else{
		printf("%d is not a prime number",num);
	}

}
