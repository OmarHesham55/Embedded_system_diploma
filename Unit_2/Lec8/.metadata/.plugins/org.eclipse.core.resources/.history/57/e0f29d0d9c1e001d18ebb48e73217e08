#include <stdio.h>
#include <string.h>
#include <conio.h>

int main(){
	char username[50];
	char name[50];
	printf("username:");
	fflush(stdin); fflush(stdout);
	fgets(username,50,stdin);
	clrscr();
	printf("Enter your name:");
	fflush(stdin); fflush(stdout);
	fgets(name,50,stdin);
	if(strcmp(username,name)){
		printf("Welcome");
	}else{
		printf("the name doesn't match with username");
	}
}
