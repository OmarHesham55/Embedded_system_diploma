#include <stdio.h>
#include <string.h>

int main(){
	int mainArr[10];
	int reverseArr[10];
	int i,j,n;
	printf("how many numbers you will enter:");
	fflush(stdin); fflush(stdout);
	scanf("%d",&n);
	printf("Enter the numbers");
	for(i=0;i<n;i++){
		fflush(stdin); fflush(stdout);
		scanf("%d",&mainArr[i]);
	}
	for(j=0;j<strlen(mainArr);j++){
		printf("%d ",mainArr[j]);
	}
}
