#include <stdio.h>

void powerNum(int n){
	if(n==1){
		printf("Enter another number except 1&0");
	}else{
		while(n!=0 && n!=1){
			n/=3;
		}
	}

	printf("the Num:%d is power of 3",n);

}

int main(){
	int n;
	printf("Enter number:");
	fflush(stdin); fflush(stdout);
	scanf("%d",&n);
	poweNum(n)

}
